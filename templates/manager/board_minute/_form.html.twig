{{ form_start(form, {attr: {novalidate: 'novalidate'}}) }}
<div class="mb-3">
    <div class="row">
        <div class="col-md-4">
            {{ form_row(form.category) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.target) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.targetCode) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.title) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.year) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.month) }}
        </div>
        <div class="col-12">
            <label for="">
                Fichier(s)
            </label>
            {{ form_row(form.files) }}
        </div>

        <div class="col-lg-4 offset-lg-4 col-md-6 offset-md-3 mt-5">
            {{ form_row(form.submit) }}
        </div>
    </div>
</div>
{{ form_rest(form) }}
{{ form_end(form) }}