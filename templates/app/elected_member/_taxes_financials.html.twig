                                                    <td>{{ total.localTax|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.otherTax|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.globalAllocation|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.totalExpenses|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.personalExpenses|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.externalExpenses|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.financialExpenses|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.grants|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.housingTax|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.propertyTax|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.noPropertyTax|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.brankCredits|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.receivedGrants|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.equipmentExpenses|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.creditRefund|number_format(0, '.', ' ') }} €</td>
                                                    <td>{{ total.debtAnnuity|number_format(0, '.', ' ') }} €</td>
                                                </tr>
                                                {% for accounting in areaFinancials %}
                                                    <tr>
                                                        <td>{{ accounting.year }}</td>
                                                        <td>{{ accounting.codeInsee }}</td>
                                                        <td>{{ accounting.communeLabel }}</td>
                                                        <td>{{ accounting.departmentLabel }}</td>
                                                        <td>{{ accounting.groupingType }}</td>
                                                        <td>{{ accounting.population|number_format(0, ' ', ' ') }}</td>
                                                        <td>{{ accounting.productsTotal|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.localTax|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.otherTax|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.globalAllocation|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.totalExpenses|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.personalExpenses|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.externalExpenses|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.financialExpenses|number_format(0, '.', ' ') }} €</td>
                                                        <td>{{ accounting.grants|number_format(0, '.', ' ') }} €</td>
