                                                <th>{{ 'TOTAL DES CHARGES DE FONCTIONNEMENT'|capitalize }}</th>
                                                <th>{{ 'CHARGES DE PERSONNEL'|capitalize }}</th>
                                                <th>{{ 'ACHATS ET CHARGES EXTERNES'|capitalize }}</th>
