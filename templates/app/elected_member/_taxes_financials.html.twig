                                                <th>{{ 'SUBVENTIONS REÇUES'|capitalize }}</th>
                                                <th>{{ 'DÉPENSES D\'ÉQUIPEMENT'|capitalize }}</th>
                                                <th>{{ 'REMBOURSEMENT D\'EMPRUNTS ET DETTES ASSIMILÉES'|capitalize }}</th>
                                                <th>{{ 'ANNUITÉ DE LA DETTE'|capitalize }}</th>
                                            </tr>
                                            </thead>
                                            <tbody>
                                            {% for key,areaFinancials in taxAccountingData['areaFinancials']['data'] %}
                                                {% set total = taxAccountingData['areaFinancials']['totals'][key] %}
                                                <tr class="text-white text-left" style="background-color: #1E90FF">
                                                    <th colspan="23">
                                                        {{ total.departmentLabel|upper }}
                                                    </th>
                                                </tr>
                                                <tr class="text-white text-center" style="background-color: #BB0B0B">
                                                    <td colspan="6">{{ total.title }}</td>
